<html metal:define-macro="master" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
    <head>
      <title tal:condition="request.model_name" tal:content="model_class.plural"></title>
      <title tal:condition="request.model_name is None" i18n:domain="fa_jquery" i18n:translate="">Models index</title>
      <link rel="stylesheet" tal:attributes="href request.static_url('fa.jquery:jquery-ui/css/smoothness/jquery-ui-1.8.8.custom.css')" />
      <link rel="stylesheet" tal:attributes="href request.static_url('fa.jquery:jquery-ui/fa.jquery.min.css')" />
      <link rel="stylesheet" tal:attributes="href request.static_url('c2cgeoportail:static/build/admin.css')" />
      <link rel="stylesheet" tal:attributes="href request.static_url('project:static/build/admin.css')" />
      <script type="text/javascript">
        if (!window.App) App = {};
        App["OpenLayers.ImgPath"] = "${request.static_url('c2cgeoportail:static/app/images/ol/')}";
      </script>
      <script type="text/javascript" tal:attributes="src request.static_url('fa.jquery:jquery-ui/fa.jquery.min.js')"></script>
      <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.13/i18n/jquery-ui-i18n.min.js"></script>
      <script type="text/javascript" tal:attributes="src request.static_url('fa.jquery:jquery-ui/fa/jquery.pyramid_formalchemy.js')"></script>
      <script type="text/javascript" tal:attributes="src request.static_url('c2cgeoportail:static/build/admin.js')"></script>
      <script type="text/javascript">
        jQuery(document).ready(function() {
          $('select#models')
            .change(function() { window.location.href = $(this).val(); })
            .selectmenu({'style':'dropdown', 'menuWidth':'20%', 'width':'100%'});
        });
      </script>
      <script type="text/javascript" metal:define-slot="javascript"></script>
      <!--[if IE]>
      <style type="text/css">
            div.fa_controls span.ui-icon {
                top: 14px;
            }
      </style>
      <![endif]-->
    </head>
    <body class="${model_name}">
      <div id="content" class="ui-admin ui-widget">
        <h1 id="header" class="ui-widget-header ui-corner-all">
          <div class="breadcrumb" tal:condition="request.model_name and breadcrumb">
            <select id="models" style="display:none;" tal:content="structure breadcrumb.render(request)" />
          </div>
          <div tal:condition="request.model_name">
            <a tal:attributes="href request.fa_url(model_name)" tal:content="model_class.plural" />
            <span tal:condition="request.model_id and hasattr(request.model_class, '__unicode__')"
                  tal:replace="unicode(request.model_instance)"/>
            <span tal:condition="request.model_id and not hasattr(request.model_class, '__unicode__')"
                  tal:replace="request.model_id"/>
          </div>
          <div tal:condition="not request.model_name" i18n:domain="fa_jquery" i18n:translate="">Models index</div>
          <div tal:condition="request.model_name" class="breadcrumb">
            <a i18n:domain="fa_jquery" i18n:translate="" tal:attributes="href request.route_url('admin')">Models index</a>
          </div>
        </h1>
        <div metal:define-slot="main">
        </div>
        <a style="display:none" class="root_url" tal:attributes="href request.fa_url()"></a>
      </div>
    </body>
</html>
